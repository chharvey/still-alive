extends _docs.tpl.pug

block append vars
  - var page = Docs.DOCS.find('/docs/obj.html')

block append toc
  p.o-Box.c-Alert.c-Alert--help.
    #[strong.c-Alert__Title Note:]
    Backgrounds have been added to all Objects on this page to show structure.

block append main
  +newSection(2, Docs.DOCS.find('/docs/obj.html#box-object'))
    include _includes/_obj.box-object.pug
  +newSection(2, Docs.DOCS.find('/docs/obj.html#textbox-object'))
    include _includes/_obj.textbox-object.pug
  +newSection(2, Docs.DOCS.find('/docs/obj.html#golden-container'))
    include _includes/_obj.golden-container.pug
    +newSection(3, Docs.DOCS.find('/docs/obj.html#flipped-golden-container'))
      include _includes/_obj.flipped-golden-container.pug
