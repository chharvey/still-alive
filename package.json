{
  "name": "still-alive",
  "version": "1.2.0",
  "description": "Theme and Style Guide for my website, chharvey.github.io",
  "private": true,
  "main": "index.js",
  "scripts": {
    "pug:docs": "pug -O docs/_models/options.js docs/{index,phi,code,principles,base,obj,comp-typo,comp-ui,help,atom}.jade",
    "lessc:docs": "lessc  docs/styles/docs.less  docs/styles/docs.css",
    "lessc:core": "lessc  still-alive.less  still-alive.css",
    "browserify:principles": "browserify docs/scripts/principles.js > docs/scripts/principles.b.js",
    "build": "npm run pug:docs  &&  npm run lessc:docs  &&  npm run lessc:core  &&  npm run browserify:principles",
    "unbuild": "rm docs/{index,phi,code,principles,base,obj,comp-typo,comp-ui,help,atom}.html {docs/styles/docs,still-alive}.css docs/scripts/principles.b.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chharvey/still-alive.git"
  },
  "keywords": [
    "theme",
    "style",
    "guide"
  ],
  "author": "Chris Harvey <chrisharvey2pi@gmail.com> (https://chharvey.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chharvey/still-alive/issues"
  },
  "homepage": "https://github.com/chharvey/still-alive#readme",
  "devDependencies": {
    "csscolor": "^1.3.0",
    "less": "^2.7.1",
    "lux-nox": "^1.2.0",
    "pug-cli": "^1.0.0-alpha6",
    "sitepage": "^1.0.0",
    "xmeter": "^5.3.0"
  }
}
